    <string name="description_us_tv_v">Dhunë</string>
    <string name="description_us_tv_y">Ky program është planifikuar që të jetë i përshtatshëm për të gjithë fëmijët.</string>
    <string name="description_us_tv_y7">Ky program është planifikuar për fëmijët e moshës 7 vjeç e lart.</string>
